<!DOCTYPE html>
<html>
<head>
    <title></title>
    <script src="javascripts/Chart.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>



</head>
<body>
<div style="width:100%">
    <div>
        <canvas id="canvas" height="450" width="1200"></canvas>

    </div>
</div>
</body>
</html>
<script>

    var lineChartData = <%-result%>

            window.onload = function(){
                var ctx = document.getElementById("canvas").getContext("2d");
                window.myLine = new Chart(ctx).Line(lineChartData, {
                    responsive: true
                });
            }

    var socket = io.connect('http://112.124.61.22:3000');
    socket.on('connect', function(){

        console.info("i connection")
        socket.on('mem_info', function(data){
            console.info("data:",data)
            myLine.addData(data.array, data.label);


        });



    });



</script>